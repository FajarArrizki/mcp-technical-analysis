#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/GEARTRADE/node_modules/.pnpm/@nullshot+cli@0.2.5/node_modules/@nullshot/cli/dist/node_modules:/root/GEARTRADE/node_modules/.pnpm/@nullshot+cli@0.2.5/node_modules/@nullshot/cli/node_modules:/root/GEARTRADE/node_modules/.pnpm/@nullshot+cli@0.2.5/node_modules/@nullshot/node_modules:/root/GEARTRADE/node_modules/.pnpm/@nullshot+cli@0.2.5/node_modules:/root/GEARTRADE/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/GEARTRADE/node_modules/.pnpm/@nullshot+cli@0.2.5/node_modules/@nullshot/cli/dist/node_modules:/root/GEARTRADE/node_modules/.pnpm/@nullshot+cli@0.2.5/node_modules/@nullshot/cli/node_modules:/root/GEARTRADE/node_modules/.pnpm/@nullshot+cli@0.2.5/node_modules/@nullshot/node_modules:/root/GEARTRADE/node_modules/.pnpm/@nullshot+cli@0.2.5/node_modules:/root/GEARTRADE/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nullshot/cli/dist/cli.js" "$@"
else
  exec node  "$basedir/../@nullshot/cli/dist/cli.js" "$@"
fi
